<div class="header">
    <h1>Artist List</h1>
</div>
<div>
    <table>
        <tr>
            <th>Name</th>
            <th>Status</th>
            <th>Pollstar</th>
            <th>Artist Site</th>
        </tr>
        <% @artists.each do |artist| %>
            <tr>
                <td><%= link_to(artist.ListName, artist_path(artist.id)) %></td>
                <td><%= link_to(artist.client_status, edit_artist_path(artist.id)) %></td>
                <td><%= link_to("pollstar #{artist.ListName}", artist.Url) %></td>
                <td></td>
            </tr>
        <% end %>
    </table>
</div>
