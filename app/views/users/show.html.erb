<h1>This is the user show page for <%= @user.fname %></h1>
<div class="row">
	<div>
		<%= link_to "Search for Artist", search_artist_path %>
		<p>
		<% @artists.each do |artist| %>
			<div>
				<%= link_to(artist.ListName, artist_path(artist)) %> - <%= artist.client_status %>
			</div>
		<% end %>
		</p>
		<div >
	  	<h1>Calendar</h1>
	  	<p>https://gist.github.com/jamesu/54116</p>
	  	<p>this is the calendar i want to use</p>
		</div>
		<div >
			<% @events.each do |event| %>
			<p>
				<%= event.PlayDate %> - <%= link_to(event.artists[0].ListName, artist_path(event.artists[0])) %> - <%= event.VenueName %> - <%= event.Region %>
			</p>
			<% end %>
		</div>
	</div>
</div>
