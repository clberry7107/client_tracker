<h1>Calendar for all tracked Artists</h1>
<div class="row">
	<div id="side_bar">
		<h1>Artists</h1>
		<%= link_to "Search for Artist", search_artist_path %><br/>
		<%= link_to("City List", cities_path) %>
		<hr>
		<p>
		<% @artists.each do |artist| %>
			<div>
				<p><%= link_to(artist.ListName, artist_path(artist)) %></p>
			</div>
		<% end %>
		</p>
	</div>
	<div id="main_body">
		<%= render 'shared/event_list', events: @events %>
	</div>
</div>
