<div class="header">
	<h1>City Index</h1>
</div>
<div class="row">
	<div id="main_body">
		<% @cities.each do |city|%>
			<% if city.State != @state %>
				</ul>
				<hr>
				<ul><%= city.State %>
			<% end %>
			<li><%= link_to(city.CityName, city_path(city)) %></li>
			<% @state = city.State %>
		<% end %>
		</ul>
	</div>
</div>